{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.company.com/jobSearch.schema.json",
    "type": "object",
    "properties": {
      "category": {
        "type": "string",
        "anyOf": [
          { "type": "string", "pattern": "^Danger" },
          { "type": "string", "pattern": "^Caution" },
          { "type": "string", "pattern": "^Park Closure" },
          { "type": "string", "pattern": "^Information" }
        ]
      },
      "parkName": {
        "type": "string",
        "minLength": 4,
        "maxLength": 4
      }    
    },
    "additionalProperties": false,
    "required": []
  }
  