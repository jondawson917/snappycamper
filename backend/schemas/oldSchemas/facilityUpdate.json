{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.campsite.com/campSearch.schema.json",
  "type": "object",
  "properties": {
    "parkCode": {
      "type": "string",
      "minLength": 4,
      "maxLength": 4
    },

    "toilets": {
      "type": "string"
    },
    "cellphonereception": {
      "type": "string",
      "anyOf": [{ "pattern": "^Yes" }, { "pattern": "^No" }]
    },
    "rv_access": {
      "type": "boolean"
    },
    "boat_access": {
      "type": "boolean"
    }
  },
  "additionalProperties": false,
  "required": ["parkName", "toilets"]
}
