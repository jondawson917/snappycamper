{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.company.com/jobSearch.schema.json",
    "type": "object",
    "properties": {
      "toiletType": {
        "type": "string",
        "anyOf": [
          { "type": "string", "pattern": "^No Toilets" },
          { "type": "string", "pattern": "^Vault Toilets" },
          { "type": "string", "pattern": "^Portable Toilets" },
          { "type": "string", "pattern": "^Flush Toilets" },
          { "type": "string", "pattern": "^Composting Toilets" }
        ]
      },
      "cellPhoneReception": {
        "type": "string",
        "anyOf": [{"type": "string", "pattern": "^Yes"}, 
                  {"type": "string", "pattern": "^No"}]},
      "rv_access": {
        "type": "boolean"
      }
    },
    "additionalProperties": false,
    "required": []
  }
  